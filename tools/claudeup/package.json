{
  "name": "claudeup",
  "version": "1.3.4",
  "description": "TUI tool for managing Claude Code plugins, MCPs, and configuration",
  "type": "module",
  "main": "dist/main.js",
  "bin": {
    "claudeup": "bin/claudeup.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "bun --watch src/main.tsx",
    "dev:tsc": "tsc -w",
    "start": "node dist/main.js",
    "start:bun": "bun run src/main.tsx",
    "lint": "npm run eslint",
    "format": "biome format --write src/",
    "eslint": "eslint \"**/*.{js,cjs,ts,mjs,jsx,tsx}\"",
    "lintfix": "npm run eslint -- --fix",
    "postlint": "template-oss-check",
    "template-oss-apply": "template-oss-apply --force",
    "snap": "tap",
    "test": "tap",
    "posttest": "npm run lint"
  },
  "keywords": [
    "claude",
    "claude-code",
    "mcp",
    "plugins",
    "tui",
    "cli",
    "ink",
    "react"
  ],
  "author": "Jack Rudenko <i@madappgang.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MadAppGang/claude-code"
  },
  "bugs": {
    "url": "https://github.com/MadAppGang/claude-code/issues"
  },
  "homepage": "https://github.com/MadAppGang/claude-code/tree/main/tools/claudeup#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "@pppp606/ink-chart": "^0.2.3",
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "ink": "^5.1.0",
    "ink-chart": "^0.1.1",
    "ink-markdown": "^1.0.4",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.18",
    "@types/semver": "^7.5.8",
    "ink-testing-library": "^4.0.0",
    "typescript": "^5.6.3"
  },
  "templateOSS": {
    "//@npmcli/template-oss": "This file is partially managed by @npmcli/template-oss. Edits may be overwritten.",
    "version": "4.25.1"
  },
  "tap": {
    "nyc-arg": [
      "--exclude",
      "tap-snapshots/**"
    ]
  }
}
